function [XY,KX,Z,Pj,Swj,Tj,MCpj,p,Q,Pw,PpW,Cw,NDT,dQ,dSS]=SimT_MKT(PR,C,A2C,G,A2G,dVC,dVG,DATA,WData,GYData,fll)
tic

KX=DATA.gKX;
KY=DATA.gKY;
KZ=DATA.gKZ;
Mp=DATA.gMp;
P=DATA.gP;
Sw=DATA.gSw;
MCp=DATA.gCp;
XY=DATA.XY;
H=DATA.gH;
Z=DATA.gZ;


Won=DATA.Won;
WonG=DATA.WonG;
WNG=DATA.WonG(:,3);
WonC=DATA.WonV;

Pw=WData.Pw;
Uf=WData.Uf;
CpW=WData.CpW;
TeW=WData.TeW;
Qz=WData.Qz;

Nl=PR.Nl;  as=PR.as; aw=PR.aw; ts=PR.ts; tw=PR.tw; mu=PR.mu;
Ta=PR.Ta;  dt=PR.dt; ndt=PR.ndt; zc=PR.zc; Bo=PR.Bo; 


Qm=zeros(size(Uf,1),5,size(Uf,2));
Qc=zeros(size(WonC,1),5,size(Uf,2));
Qg=zeros(size(WNG,1),5,size(Uf,2));
%KX(:)=mean(KX(:));

[A]=MR_Prop(XY,Nl);

% if Nl>1
%  [AT]=MR_Prop(XY,Nl-1);
%  [LTer,BTer,STer,H1Ter]=Geome3(AT,XY,Z,DATA.dH);
% end;

[L,B,S,H1]=Geome3(A,XY,Z,H);
Wf1=KWell(KX,H,S,Won);
[r,c]=find(A==1);
Doly=[0.25,0.5,0.25,0.5,1,0.5,0.25,0.5,0.25];
Wf=KWell_2(KX,H,S,L,B,Won,r,c,Doly);
% Wf
% Wf(1)=Wf(9);
XY=repmat(XY,Nl,1);

[A,L,S,B,H1,K,XY,Mp,Sw,H,Z,P,MCp,T,NTG,p,rz,cz,BXY,BZ,dH,NL,NamXY,GYData]=PereYpor(A,L,S,B,H1,KX,KY,KZ,Mp,Sw,XY,H,Z,P,MCp,DATA,GYData);
[r,c]=find(L);

nw=size(p,2);
nwc=size(A2C,1);
raz=nw-nwc;

A2C(raz+1:end+raz,:)=A2C;
A2C(1:raz,:)=0;
A2C=A2C(p,:);
A2G=A2G(p,:);

for i=1:size(Won,1)
  Won(i)=find(Won(i)==p);
end;

na=size(XY,1);  nc=size(C,1);  ng=size(G,1);    nw=size(Qz,1);
C2G=sparse(nc,ng);
C2GL=C2G;
WonM=repmat(1:nw,1,Nl);

figure(98),subplot(2,4,7),spy([A,A2C,A2G;A2C',C,C2G;A2G',C2G',G]);

Pi(1:na,1)=P;
Pi(na+1:na+nc,1)=P(1:nc);
Pi(na+nc+1:na+nc+ng,1)=P(1:ng);

Ti(1:na,1)=T;
Ti(na+1:na+nc,1)=T(1:nc);
Ti(na+nc+1:na+nc+ng,1)=T(1:ng);

[Ke,Ke_gy,dV]=KH2Mat(K,H,Mp,S,r,c,rz,cz,GYData.GY_Kz,BZ); 


Cw(:,1)=Sw(1:nc,1);
Gw(:,1)=Sw(1:ng,1);

Mc=ones(nc,1);
Mg=ones(ng,1);

CCp(:,1)=zeros(nc,1);
GCp(:,1)=zeros(ng,1);

dVCG=[dV;dVC;dVG];

[TM,TC,TG,TA2C,TA2G,RC,T_GY]=Pre_fast(A,C,A2C,A2G,G,Ke,L,B,H1,K(:,1),Ke_gy,BZ,rz,cz);
%[LM,LC,LG,LA2C,LA2G]=Pre_fast(A,C,A2C,A2G,G,1,L,B,H1,ones(size(K(:,1))));
j=0;
PpW=zeros(size(Uf));
[Img2,ST,CR_rc]=Pre_Crack(RC,na,TM,A2C,A2G,Wf,Won);

for t=1:Ta-1
    

    Qf=Qz(:,t+1);
    %% ������ ��������

    %[C,TC]=GeoMex(C11,TC1,Pi(na+1:na+nc,t),fll);
    [b1gm,b1gc,b1gg,b2gm]=GY_bild(GYData,Pi(1:na,1),Sw(:,1),BZ,na,nc,ng,T_GY,as,aw,mu);
    
    
    [TL,TW]=Potok_MKT(TM,Pi(1:na,1),Sw(:,1),MCp(:,1),as,aw,mu,RC.Arc);
    [CL,CW]=Potok_Tube(C,Pi(na+1:na+nc,1),Cw(:,t),CCp(:,t),PR);
    [GL,GW]=Potok_Tube(G,Pi(na+nc+1:na+nc+ng,1),Gw(:,t),GCp(:,t),PR);


    [A2CL,A2CW]=Obmen_T2M(A2C,Pi(1:na,1),Pi(na+1:na+nc,1),Sw(:,1),Cw(:,t),KX,PR,MCp(:,1),CCp(:,t));
    [A2GL,A2GW]=Obmen_T2M(A2G,Pi(1:na,1),Pi(na+nc+1:na+nc+ng,1),Sw(:,1),Gw(:,t),KX,PR,MCp(:,1),GCp(:,t));


    [W1,W6,W7]=Well_MKT(Wf,Won,Uf(:,t),Sw(:,1),MCp(:,1),aw,as,mu,CpW(:,t));
    [W1C,W6C,W7C]=Well_MKT(WonC(:,2),WonC(:,1),Uf(WonC(:,3),t),Cw(:,t),CCp(:,t),tw,ts,mu,CpW(WonC(:,3),t));
    [W1G,W6G,W7G]=Well_MKT(WonG(:,2),WonG(:,1),Uf(WNG,t),Gw(:,t),GCp(:,t),tw,ts,mu,CpW(WNG,t));
  
    [BA1,BC1,BG1]=SGim(Pi(:,1),na,nc,ng,zc);
    
    A1=TL-sparse(Won,Won,W1,na,na)-sparse(1:na,1:na,sum(A2CL,2)+sum(A2GL,2)+BA1+b1gm',na,na);
    C1=CL-sparse(1:nc,1:nc,sum(A2CL,1)+BC1',nc,nc)-sparse(WonC(:,1),WonC(:,1),W1C,nc,nc);
    G1=GL-sparse(1:ng,1:ng,sum(A2GL,1)+BG1',ng,ng)-sparse(WonG(:,1),WonG(:,1),W1G,ng,ng);

    PwNl=repmat(Pw(:,t+1),Nl,1);
    b1wm=sparse(Won,ones(1,size(Won,1)),-W1.*PwNl,na,1);
    b1wc=sparse(WonC(:,1),ones(1,size(WonC,1)),-W1C.*Pw(WonC(:,3),t+1),nc,1);
    b1wg=sparse(WonG(:,1),ones(1,size(WonG,1)),-W1G.*Pw(WNG,t+1),ng,1);

    W2M=sparse(WonM,Won,W1,nw,na);
    W2C=sparse(WonC(:,3),WonC(:,1),W1C,nw,nc);
    W2G=sparse(WNG,WonG(:,1),W1G,nw,ng);
%     b1wm
%      W2M
%      Qf
%      W2M
%      W2M(Qf==0,:)
     %(sum(W2M(Qf==0,:))~=0)'
      
    b1wm=b1wm.*(sum(W2M(Qf==0,:))~=0)';
    b1wc=b1wc.*(sum(W2C(Qf==0,:))~=0)';
    b1wg=b1wg.*(sum(W2G(Qf==0,:))~=0)';
   %  b1wm

    WM1=[W2M,W2C,W2G];
    WM2=WM1';
    W3vec=sparse(WonM,1,W1,nw,1)+sparse(WonC(:,3),1,W1C,nw,1)+sparse(WNG,1,W1G,nw,1);
    WM3=-sparse(1:nw,1:nw,W3vec,nw,nw);
    
    AM=[A1,A2CL,A2GL;A2CL',C1,C2GL;A2GL',C2GL',G1];
%     size(-b1gm.*GYData.GY_Pz')
%     size(b1gc)
%     size(b1gg)
    BM=[b1wm;b1wc;b1wg]'+[-b1gm.*GYData.GY_Pz',b1gc,b1gg];
%     12365
%     WM1
    
    WM1=WM1(Qf~=0,:);
    WM2=WM2(:,Qf~=0);
    WM3=WM3(Qf~=0,Qf~=0);

    Pt=[BM,Qz(Qf~=0,t+1)]/[AM,WM2;WM1,WM3];

    Pi(:,1)=Pt(1:na+nc+ng);
    Pw(Qf~=0,t+1)=Pt(na+nc+ng+1:end);

    %% ������ ������������
    SCw=[Sw(:,1);Cw(:,t);Gw(:,t)];
    Sw0=SCw;
%     size(Sw0)
%     size(dVCG)
%     fghj
    SCp=[MCp(:,1);CCp(:,t);GCp(:,t)];

 %[SCw,SCp,NDT(t)]=Sat_fast(SCw,SCp,RC,TC,TG,TA2C,TA2G,TM,Pi(:,1),PR,ndt,Won,Wf,...
 %    Uf(:,t),dt,dVCG,Pw(:,t),WonG,CpW(:,t),WonC,Nl,b2gm,GYData.GY_Pz);

 [SCw,SCp,NDT(t),Q1,Q2,Qm1,Bw,dSS(t)]=Sat_fast_2(SCw,SCp,RC,TC,TG,TA2C,TA2G,TM,Pi(:,1),PR,ndt,Won,Wf,...
     Uf(:,t),dt,dVCG,Pw(:,t+1),WonG,CpW(:,t),WonC,Nl,b2gm,GYData.GY_Pz,Img2,ST,CR_rc,Qz(:,t+1),Qf,WonM);
    
    Sw(:,1)=SCw(1:na);
    Cw(:,t+1)=SCw(na+1:na+nc);
    Gw(:,t+1)=SCw(na+nc+1:end);
    
    MCp(:,1)=SCp(1:na);
    CCp(:,t+1)=SCp(na+1:na+nc);
    GCp(:,t+1)=SCp(na+nc+1:end);
   
    Sw(:,1)=Sw(:,1).*(Sw(:,1)>=0).*(Sw(:,1)<=1)+(Sw(:,1)>1);
    Cw(:,t+1)=Cw(:,t+1).*(Cw(:,t+1)>=0).*(Cw(:,t+1)<=1)+(Cw(:,t+1)>1);
    Gw(:,t+1)=Gw(:,t+1).*(Gw(:,t+1)>=0).*(Gw(:,t+1)<=1)+(Gw(:,t+1)>1);
    

    %% ������ �������
   
    Qm(:,:,t+1)=QBild(W1,W6,W7,Pi(1:na,1),Uf(:,t+1),Won,dt,Pw(WonM,t+1));
    Qm(CR_rc.wn,:,t+1)=Qm1;
   % Qm1
    Qc(:,:,t+1)=Q1;%QBild(W1C,W6C,W7C,Pi(na+1:na+nc,1),Uf(WonC(:,3),t+1),WonC(:,1),dt,Pw(WonC(:,3),t+1));
    Qg(:,:,t+1)=Q2;%QBild(W1G,W6G,W7G,Pi(na+nc+1:end,1),Uf(WNG,t+1),WonG(:,1),dt,Pw(WNG,t+1));
    PpW(:,t+1)=Pi(Won);
        %% ������ �����������
%    Ti(:,t+1)=Termal(Ti(:,t),Pi(:,t+1),SCw,Mp,Mc,Mg,NTG,LM,LC,LG,LA2C,LA2G,PR,RC,dVCG,dt,Won,WonM,WonV,WonG,...
 %       TeW(:,t+1),Qm(:,:,t+1),Qc(:,:,t+1),Qg(:,:,t+1),C2GL,TL,CL,GL,TW,CW,GW,A2CL,A2CW,A2GL,A2GW,Wf,NDT(t),t,T,S,BZ);
    
        Ti(:,1)=Ti(:,1);
    if mod(t,1)==0
        j=j+1;
        Pj(:,j)=Pi;
        Swj(:,j)=Sw;
        MCpj(:,j)=MCp;
        Tj(:,j)=Ti;
    end;
    sQo=sum(Qm(:,2,t+1)-Qm(:,3,t+1)+Qm(:,1,t+1))+sum(Qc(:,2,t+1)-Qc(:,3,t+1)+Qc(:,1,t+1))+sum(Qg(:,2,t+1)-Qg(:,3,t+1)+Qg(:,1,t+1));
   dQ(t)=sum(Sw0.*[dV;dVc.dVg])-sum([Sw;Cw(:,t+1);Gw(:,t+1)].*[dV;dVc;dVg])-sum(sQo(:))+Bw;
end;
% MCp(MCp(:,end)~=0,:)
 %Sw(Sw(:,end)~=1,:)
% [r,c]=find(MCp(:,end)~=0)
% CCp
% sum(Qm,1)
% sum(Qc,1)
for i=1:5
    qcs=size(Qc(:,i,:));
    QM(:,:)=Qm(:,i,:);
    QC(1:qcs(1),1:qcs(3))=Qc(:,i,:);
    
    qcs=size(Qg(:,i,:));
    QG(1:qcs(1),1:qcs(3))=Qg(:,i,:);

    Q(:,i,:)=[QM;QC;QG];
    QC1=zeros(size(QM));
    QG1=zeros(size(QM));
    QC1(WonC(:,3),:)=QC;
    QG1(WNG,:)=QG;
    QW(:,i,:)=QM+QC1+QG1;
end;


%ert(:,:)=QW(:,1,:);
%ert1(:,:)=QW(:,2,:);
%ert(ert(:,end)~=0,:)
%sum(ert1,1)
%Qz

% max(SDF,[],1)
% max(SDF1,[],1)
%sum(SDF(:)>1)
%sum(SDF1(:)>1)

sum(NDT)-Ta
toc